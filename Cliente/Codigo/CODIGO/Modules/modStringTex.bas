Public Function RemakeMsg(ByVal msg As String) As String
On Error Resume Next
If cMsgLoaded = False Then Exit Function
Dim i As Integer
RemakeMsg = msg
For i = 0 To MaxMsg
    If InStrB(RemakeMsg, lMsg(i).key) <> 0 Or RemakeMsg = lMsg(i).key Then
        RemakeMsg = Replace(RemakeMsg, lMsg(i).key, lMsg(i).msg)
    End If
Next
End Function
